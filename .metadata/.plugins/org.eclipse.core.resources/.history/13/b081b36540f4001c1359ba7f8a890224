package Review;

public class Fibonacci {

	public static void main(String[] args) {
		int num=10;
		int n1=0;
		int n2=1;
		int n3=0;
		System.out.println(n1);
		System.out.println(n2);
		for(int i=2; i<num; i++) {
			n1=n3+n2;
			System.out.println(n1);
			
			n1=n2;
			n2=n3;
		}
		
		// TODO Auto-generated method stub

	}

}
